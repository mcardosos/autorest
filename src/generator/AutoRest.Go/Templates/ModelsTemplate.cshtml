@using System;
@using System.Collections.Generic;
@using System.Linq;

@using AutoRest.Go
@using AutoRest.Core.Model
@using AutoRest.Go.Model
@using AutoRest.Go.Templates
@using AutoRest.Core.Utilities

@inherits AutoRest.Core.Template<AutoRest.Go.Model.CodeModelGo>
@{
    var imports = Model.ModelImports;
}
package @Model.PackageName
@EmptyLine
@Header("// ")

@EmptyLine

@if (!imports.IsNullOrEmpty())
{
@:import (
foreach (var import in imports)
{
    @:"@(import)"
}
@:)    
@EmptyLine
}

@foreach (var enumTemplateModel in Model.EnumTemplateModels) {
@:@(Include(new EnumTemplate(), enumTemplateModel))
@EmptyLine
@:
}

@foreach (var modelTemplateModel in Model.ModelTemplateModels) {
@:@(Include(new ModelTemplate(), modelTemplateModel))
@EmptyLine
@:
}
